<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="screen" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.winternote.winternote.controller.CreationController">
    <Text layoutX="256.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" textAlignment="CENTER"
          wrappingWidth="87.4609375">
        <font>
            <Font size="24.0"/>
        </font>
    </Text>
    <TextField fx:id="title" layoutX="187.0" layoutY="91.0" prefHeight="25.0" prefWidth="226.0"/>
    <Text layoutX="256.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Location" textAlignment="CENTER"
          wrappingWidth="87.4609375">
        <font>
            <Font size="24.0"/>
        </font>
    </Text>
    <HBox prefHeight="0.0" prefWidth="226.0" alignment="CENTER">
        <TextField fx:id="path" layoutX="187.0" layoutY="91.0" prefHeight="25.0" prefWidth="226.0"/>
        <Button mnemonicParsing="false" text="Browse" onAction="#onBrowseButtonClick"/>
    </HBox>
    <HBox fx:id="buttonBox">
        <Button layoutX="197.0" layoutY="175.0" mnemonicParsing="false" onAction="#onCreateButtonClick" text="Create"/>
        <Button layoutX="348.0" layoutY="175.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel"/>
    </HBox>
</VBox>
